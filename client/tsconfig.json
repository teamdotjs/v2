{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "jsx": "react",
        "lib": ["dom", "es6", "es2015.promise"],
        "types": ["whatwg-fetch", "material-ui", "history"]
    },
    "include": [
        "src/**/*.{tsx,ts}"
    ]
}
